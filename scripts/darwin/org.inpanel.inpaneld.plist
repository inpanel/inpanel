<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>org.inpanel.inpaneld</string>

        <key>WorkingDirectory</key>
        <string>/usr/local/inpanel</string>

        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/inpanel/inpaneld</string>
        </array>

        <key>StandardOutPath</key>
        <string>/var/log/inpanel/main.log</string>

        <key>StandardErrorPath</key>
        <string>/var/log/inpanel/error.log</string>

        <key>RunAtLoad</key>
        <false/>

        <key>KeepAlive</key>
        <true/>
    </dict>
</plist>